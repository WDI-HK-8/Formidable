app.controller('SubmissionsIndexCtrl', ['$scope', '$http','$auth','$location','$routeParams', function($scope, $http, $auth, $location, $routeParams){
  
  url = "<%= ENV['URL'] %>" || 'http://localhost:3000'

  $http.get(url + '/forms/' + $routeParams.id + '/submissions').success(function(response) {
    console.log(response)
    if(!response.length) {
      $scope.noSubmissions = true;
    }
  });
}]);